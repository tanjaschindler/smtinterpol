(set-option :produce-proofs true)
(set-option :produce-interpolants true)
(set-option :interpolant-check-mode true)
(set-logic QF_LRA)
(declare-fun x1 () Real)
(declare-fun x2 () Real)
(assert (! (and (not (< (+ x1 (* 2 x2)) 1)) (not (< (+ (* 2 x1) x2) 1))) :named A))
(assert (! (< (+ x1 x2) 0.5) :named B))
(check-sat)
(get-proof)
(get-interpolants A B)