(set-option :produce-proofs true)
(set-option :produce-interpolants true)
(set-option :interpolant-check-mode true)
(set-logic QF_LRA)
(declare-fun x () Real)
(declare-fun y1 () Real)
(declare-fun y2 () Real)
(assert (! (and (> (+ x y1) 0) (> (+ x y2) 0) (> (* (- 1) x) 0)) :named A))
(assert (! (>= (+ (* (- 2) y1) (* (- 5) y2)) 0) :named B))
(check-sat)
(get-proof)
(get-interpolants A B)